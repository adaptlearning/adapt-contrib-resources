{
  "type":"object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "id": "http://jsonschema.net",
  "required":false,
  "properties":{
    "pluginLocations": {
      "type":"object",
      "required":true,
      "properties":{
        "config": {
          "type":"object"
        },
        "course": {
          "type":"object",
          "properties":{
            "_resources": {
              "type":"object",
              "required":true,
              "properties":{
                "title": {
                  "type":"string",
                  "title": "Resource title"
                },
                "description": {
                  "type":"string",
                  "title": "Description"
                },
                "_isEnabled": {
                  "type":"boolean",
                  "required":true,
                  "title": "Enabled"
                },
                "_filterButtons": {
                  "type":"array",
                  "required":true,
                  "title": "Filter buttons",
                  "minItems": 1,
                  "items":
                  {
                    "type":"object",
                    "required":true,
                    "properties":{
                      "name": {
                        "type":"string",
                        "required":true,
                        "title": "Resource type",
                        "minLength": 1
                      },
                      "displayName": {
                        "type":"string",
                        "required":true,
                        "title": "Display name",
                        "minLength": 1
                      }
                    }
                  }
                },
                "_resourcesItems": {
                  "type":"array",
                  "required":true,
                  "title": "Resource items",
                  "items":
                  {
                    "type":"object",
                    "required":true,
                    "properties":{
                      "_link": {
                        "type":"string",
                        "title": "Link"
                      },
                      "_type": {
                        "type":"string",
                        "required":true,
                        "title": "Type",
                        "minLength": 1
                      },
                      "description": {
                        "type":"string",
                        "title": "Description"
                      },
                      "title": {
                        "type":"string",
                        "required":true,
                        "title": "Title",
                        "minLength": 1
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "contentobject": {
          "type":"object"
        },
        "article": {
          "type":"object"
        },
        "block": {
          "type":"object"
        },
        "component": {
          "type":"object"
        }
      }
    }
  }
}
